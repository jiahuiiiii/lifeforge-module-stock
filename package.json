{
  "name": "@lifeforge/jiahuiiiii--stock",
  "displayName": "Stock",
  "version": "0.0.4",
  "description": "Track stocks, manage your watchlist, build virtual portfolio, and analyze stocks with the Cold Eye Investment Strategy.",
  "author": "jiahuiiiii <wongjiahui0915@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jiahuiiiii/lifeforge-module-stock"
  },
  "scripts": {
