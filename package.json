    "zustand": "^5.0.10"
  },
  "peerDependencies": {
    "lifeforge-ui": "workspace:*",
    "shared": "workspace:*",
    "@lifeforge/server-utils": "workspace:*"
  },
  "devDependencies": {
    "@types/uuid": "^11.0.0"
  },
  "lifeforge": {
    "category": "Finance",
    "icon": "material-symbols-light:candlestick-chart",
    "APIKeyAccess": {
      "fmp": {
        "required": true,
        "usage": "Fetching stock quotes"
      }
    }
